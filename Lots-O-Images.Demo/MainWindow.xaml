<Window x:Class="Lots_O_Images.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:v="clr-namespace:Lots_O_Images;assembly=Lots-O-Images"
        xmlns:local="clr-namespace:Lots_O_Images.Demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    
    <Window.DataContext>
        <local:MainViewModel></local:MainViewModel>
    </Window.DataContext>
    <Grid>
        <v:ImageViewer ItemsSource="{Binding DynamicImages}"></v:ImageViewer>
    </Grid>

</Window>
